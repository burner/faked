	override string dateWeekday() {
		final switch(uniform(0, 10, this.rnd)) {
			case 0: return "abbr: null";
			case 1: return "wide: [";
			case 2: return "الأحَد";
			case 3: return "الإثنين";
			case 4: return "الثلاثاء";
			case 5: return "الأربعاء";
			case 6: return "الخميس";
			case 7: return "الجمعة";
			case 8: return "السبت";
			case 9: return "]";
		}
	}


	override string dateMonth() {
		final switch(uniform(0, 28, this.rnd)) {
			case 0: return "wide: [";
			case 1: return "كَانُون ٱلثَّانِي";
			case 2: return "شُبَاط";
			case 3: return "آذَار";
